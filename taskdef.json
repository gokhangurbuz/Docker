{
    "family": "staging",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "development-container",
            "cpu": 10,
            "memory": 400,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort":8080
                }
            ],
            "privileged": false
        }
    ]
}
